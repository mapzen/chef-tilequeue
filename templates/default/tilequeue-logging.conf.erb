[loggers]
keys=root,process,seed,intersect,drain,prune_tiles_of_interest,enqueue_tiles_of_interest,wof_process_neighbourhoods

[handlers]
keys=consoleHandler,consoleTimestampHandler

[formatters]
keys=simpleFormatter,timestampFormatter

[logger_root]
level=WARNING
handlers=consoleHandler

[logger_prune_tiles_of_interest]
level=INFO
handlers=consoleTimestampHandler
qualName=prune_tiles_of_interest
propagate=0

[logger_enqueue_tiles_of_interest]
level=INFO
handlers=consoleTimestampHandler
qualName=enqueue_tiles_of_interest
propagate=0

[logger_process]
level=INFO
handlers=consoleHandler
qualName=process
propagate=0

[logger_seed]
level=INFO
handlers=consoleTimestampHandler
qualName=seed
propagate=0

[logger_intersect]
level=INFO
handlers=consoleTimestampHandler
qualName=intersect
propagate=0

[logger_drain]
level=INFO
handlers=consoleTimestampHandler
qualName=drain
propagate=0

[logger_wof_process_neighbourhoods]
level=INFO
handlers=consoleTimestampHandler
qualName=wof_process_neighbourhoods
propagate=0

[handler_consoleHandler]
class=StreamHandler
formatter=simpleFormatter
args=(sys.stdout,)

[handler_consoleTimestampHandler]
class=StreamHandler
formatter=timestampFormatter
args=(sys.stdout,)

[formatter_simpleFormatter]
format=%(name)s - %(levelname)s - %(message)s
datefmt=

[formatter_timestampFormatter]
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s
datefmt=
